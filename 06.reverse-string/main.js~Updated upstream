/**
 * @param { string } str
 * @return { string }
 */
function reverseString(str) {
    let res = str.split("");
    let sumString = "";
    // const reverseArr = []

    for(let i = res.length-1;i >= 0 ; i--){
    // reverseArr.push(res[i])

         sumString +=  res[i];

    }
    // console.log(reverseArr)

    return sumString;

    // return reverseArr.join('')
}

reverseString('Code Camp 8')

module.exports = reverseString
